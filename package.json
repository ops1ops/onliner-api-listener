{
  "name": "onliner-api-listener",
  "version": "1.0.0",
  "description": "https://onliner-api-listener.herokuapp.com",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "server": "babel-node -r dotenv/config src/server/index.js",
    "start": "npm run build && npm run server",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server:dev": "nodemon --exec npm run server",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "onliner-listener": "babel-node src/server/onliner-listener.js",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
